Checks = (CheckIn, CheckOut)

getAvailableRooms :: GET => Request {Checks} -> Response {[Room]}
isRoomAvailable :: GET => Request {Room.id, Checks} -> Response {Bool}
areRoomsAvailables :: GET => Request {[Room.id], Checks} -> Response {Bool}

getPeriodicReport :: GET => Request {Checks} -> Response {Report: {Total, Reserved, Remaining}}

Booking :: POST => Request {ReservationData} -> Response {Reservation}
Cancel :: PATCH/PUT => Request {Reservation.id} -> Response Reservation

